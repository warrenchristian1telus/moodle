{
  "name": "bcdevops/moodle",
  "description": "Moodle installation for Docker to OpenShift Build",
  "license": "GPL-3.0",
  "type": "composer-plugin",
  "authors": [
    {
      "name": "BCDevOps"
    }
  ],
  "config": {
    "github-oauth": {
      "github.com": "${GITHUB_AUTH_TOKEN}"
    }
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "${GIT_MOODLE_URL}"
    }
  ],
  "require": {
    "moodle/moodle": "${MOODLE_VERSION}"
  }
}
